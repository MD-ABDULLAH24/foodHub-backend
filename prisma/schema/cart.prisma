model Cart {
  id        String   @id @default(uuid())
  userId    String
  mealId    String
  quantity  Int      @default(1)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  meal Meals @relation(fields: [mealId], references: [id], onDelete: Cascade)

  @@unique([userId, mealId])
  @@index([userId])
  @@index([mealId])
}
